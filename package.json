{
  "name": "angular-bootstrap-md",
  "version": "7.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-scss": "cp -r projects/angular-bootstrap-md/src/lib/scss/ projects/angular-bootstrap-md/src/lib/img/ projects/angular-bootstrap-md/src/lib/font/ dist/angular-bootstrap-md",
    "copy-files": "cp projects/angular-bootstrap-md/src/lib/changelog README.md dist/angular-bootstrap-md",
    "pack-archive": "cd dist/angular-bootstrap-md && npm pack . && cp angular-bootstrap-md-7.*.tgz ../..",
    "remove-archive": "rm dist/angular-bootstrap-md/angular-bootstrap-md-7.*.tgz && cd ../..",
    "compile-lib": "ng build angular-bootstrap-md && npm run copy-scss && npm run copy-files && npm run pack-archive && npm run remove-archive",
    "schematics:build": "tsc -p projects/schematics/tsconfig.json",
    "schematics:copy": "npm run schematics:build && webpack",
    "compile": "npm run compile-lib && npm run schematics:copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@types/chart.js": "^2.7.42",
    "chart.js": "^2.5.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "angular-bootstrap-md": "file:angular-bootstrap-md-7.5.1.tgz"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.3",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular-devkit/core": "^7.2.2",
    "@angular-devkit/schematics": "^7.2.2",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "schematics-utilities": "^1.1.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}